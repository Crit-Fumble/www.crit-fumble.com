{
    "name": "@crit-fumble/core",
    "version": "6.8.2",
    "description": "Shared data for Crit-Fumble applications; version matches prisma client version",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "lint": "eslint .",
        "test": "jest",
        "pull": "npx dotenv-cli -e ../../.env prisma db pull",
        "generate": "npx dotenv-cli -e ../../.env prisma generate",
        "migrate": "npx dotenv-cli -e ../../.env prisma migrate dev",
        "deploy": "npx dotenv-cli -e ../../.env prisma migrate deploy",
        "reset": "npx dotenv-cli -e ../../.env prisma migrate reset",
        "push": "npx dotenv-cli -e ../../.env prisma db push",
        "seed": "npx dotenv-cli -e ../../.env prisma db prisma/seed"
    },
    "keywords": [
        "crit-fumble",
        "shared",
        "data"
    ],
    "author": "Crit-Fumble",
    "license": "MIT",
    "dependencies": {
        "@crit-fumble/discord": "^14.22.1",
        "@crit-fumble/openai": "^5.16.0-r2",
        "@crit-fumble/worldanvil": "^2.0.0-r2",
        "@dice-roller/rpg-dice-roller": "^5.5.1",
        "@napi-rs/canvas": "^0.1.44",
        "@prisma/client": "^6.8.2",
        "@types/jsonwebtoken": "^9.0.10",
        "@vercel/blob": "^1.1.1",
        "chrono-node": "^2.8.4",
        "dotenv": "^16.3.1",
        "dotenv-cli": "^10.0.0",
        "jsonwebtoken": "^9.0.2",
        "node-fetch": "^3.3.2",
        "pdfjs-dist": "^5.4.149"
    },
    "devDependencies": {
        "@babel/core": "^7.28.3",
        "@babel/preset-env": "^7.28.3",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.17.24",
        "eslint": "^8",
        "jest": "^29.7.0",
        "prisma": "^6.8.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    }
}
