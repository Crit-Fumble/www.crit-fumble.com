{
    "name": "@crit-fumble/core",
    "version": "6.16.1",
    "description": "Shared data for Crit-Fumble applications; version matches prisma client version",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "lint": "eslint .",
        "test": "jest",
        "pull": "npx dotenv-cli -e ../../.env prisma db pull",
        "pull:prod": "npx dotenv-cli -e ../../.env.prod prisma db pull --print > scripts/prod-schema-inspect.prisma",
        "inspect:prod": "npx dotenv-cli -e ../../.env.prod prisma db pull --print",
        "generate": "npx dotenv-cli -e ../../.env prisma generate",
        "generate:prod": "npx dotenv-cli -e ../../.env.prod prisma generate",
        "migrate": "npx dotenv-cli -e ../../.env prisma migrate dev",
        "migrate:prod": "npx dotenv-cli -e ../../.env.prod prisma migrate deploy",
        "deploy": "npx dotenv-cli -e ../../.env prisma migrate deploy",
        "deploy:prod": "npx dotenv-cli -e ../../.env.prod prisma migrate deploy",
        "reset": "npx dotenv-cli -e ../../.env prisma migrate reset",
        "reset:prod": "npx dotenv-cli -e ../../.env.prod prisma migrate reset --force --skip-generate",
        "push": "npx dotenv-cli -e ../../.env prisma db push",
        "push:prod": "npx dotenv-cli -e ../../.env.prod prisma db push --accept-data-loss",
        "reset-force:prod": "npx dotenv-cli -e ../../.env.prod prisma db push --force-reset --accept-data-loss",
        "seed": "npx dotenv-cli -e ../../.env prisma db prisma/seed",
        "seed:prod": "npx dotenv-cli -e ../../.env.prod prisma db prisma/seed",
        "studio": "npx dotenv-cli -e ../../.env prisma studio",
        "studio:prod": "npx dotenv-cli -e ../../.env.prod prisma studio"
    },
    "keywords": [
        "crit-fumble",
        "shared",
        "data"
    ],
    "author": "Crit-Fumble",
    "license": "MIT",
    "dependencies": {
        "@crit-fumble/worldanvil": "^2.0.0-r2",
        "@dice-roller/rpg-dice-roller": "^5.5.1",
        "@napi-rs/canvas": "^0.1.44",
        "@prisma/client": "^6.16.1",
        "@types/jsonwebtoken": "^9.0.10",
        "@vercel/blob": "^1.1.1",
        "chrono-node": "^2.8.4",
        "discord.js": "^14.22.1",
        "dotenv": "^16.3.1",
        "dotenv-cli": "^10.0.0",
        "effect": "^3.17.13",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "node-fetch": "^3.3.2",
        "openai": "^5.16.0",
        "pdfjs-dist": "^5.4.149",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@babel/core": "^7.28.3",
        "@babel/preset-env": "^7.28.3",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.12",
        "@types/node": "^20.17.24",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8",
        "eslint-plugin-import": "^2.29.1",
        "jest": "^29.7.0",
        "prisma": "^6.16.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    }
}
