{
  "name": "@crit-fumble/openai",
  "version": "0.1.0",
  "description": "OpenAI API integration for Crit-Fumble",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint '{models,server,client}/**/*.ts'",
    "clean": "rimraf dist"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models": {
      "import": "./dist/models/index.js",
      "types": "./dist/models/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "keywords": [
    "crit-fumble",
    "openai",
    "ai"
  ],
  "author": "Crit-Fumble",
  "license": "MIT",
  "dependencies": {
    "@crit-fumble/core": "workspace:^6.8.2",
    "openai": "^5.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "eslint": "^8",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.2"
  }
}
