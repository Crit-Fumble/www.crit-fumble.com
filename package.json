{
  "name": "crit-fumble-web",
  "version": "0.2.0",
  "private": true,
  "type": "commonjs",
  "workspaces": [
    "packages/*",
    "agents/*"
  ],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:core-only && next build",
    "build:core-only": "npm run generate --workspace=@crit-fumble/core && npm run build --workspace=@crit-fumble/core",
    "build:all-packages": "npm run build --workspace=@crit-fumble/core && npm run build --workspace=@crit-fumble/worldanvil",
    "start": "next start",
    "postbuild": "",
    "lint": "next lint",
    "build:packages": "npm run build --workspaces --if-present",
    "build:all": "tsc -b",
    "build:workspaces": "npm run build --workspaces --if-present",
    "build:optimized": "node scripts/build-optimized.js build",
    "build:clean": "tsc -b --clean",
    "build:force": "tsc -b --force",
    "build:watch": "tsc -b --watch",
    "build:verbose": "node scripts/build-optimized.js build --verbose",
    "clean": "find ./packages -type d -name dist -exec rm -rf {} +",
    "clean:cache": "node scripts/build-optimized.js clean --cache",
    "dev:packages": "npm run dev --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint:packages": "npm run lint --workspaces --if-present",
    "lint:all": "npm run lint --workspaces",
    "create-package": "node scripts/create-package.js",
    "create-component": "node scripts/create-component.js",
    "agent:discord:build": "npm run build --workspace=crit-fumble-discord-agent",
    "agent:discord:dev": "npm run dev --workspace=crit-fumble-discord-agent",
    "agent:discord:start": "npm run start --workspace=crit-fumble-discord-agent",
    "agent:discord:deploy": "npm run deploy --workspace=crit-fumble-discord-agent",
    "agent:discord:lint": "npm run lint --workspace=crit-fumble-discord-agent",
    "agent:discord:test": "npm run test --workspace=crit-fumble-discord-agent",
    "agents:build": "npm run build --workspaces=agents/*",
    "agents:dev": "npm run dev --workspaces=agents/*",
    "agents:lint": "npm run lint --workspaces=agents/*",
    "agents:test": "npm run test --workspaces=agents/*",
    "db:pull": "npm run pull --workspace=@crit-fumble/core",
    "db:pull:prod": "npm run pull:prod --workspace=@crit-fumble/core",
    "db:inspect:prod": "npm run inspect:prod --workspace=@crit-fumble/core",
    "db:generate": "npm run generate --workspace=@crit-fumble/core",
    "db:generate:prod": "npm run generate:prod --workspace=@crit-fumble/core",
    "db:migrate": "npm run migrate --workspace=@crit-fumble/core",
    "db:migrate:prod": "npm run migrate:prod --workspace=@crit-fumble/core",
    "db:deploy": "npm run deploy --workspace=@crit-fumble/core",
    "db:deploy:prod": "npm run deploy:prod --workspace=@crit-fumble/core",
    "db:reset": "npm run reset --workspace=@crit-fumble/core",
    "db:reset:prod": "npm run reset:prod --workspace=@crit-fumble/core",
    "db:push": "npm run push --workspace=@crit-fumble/core",
    "db:push:prod": "npm run push:prod --workspace=@crit-fumble/core",
    "db:seed": "npm run seed --workspace=@crit-fumble/core",
    "db:seed:prod": "npm run seed:prod --workspace=@crit-fumble/core",
    "db:studio": "npm run studio --workspace=@crit-fumble/core",
    "db:studio:prod": "npm run studio:prod --workspace=@crit-fumble/core",
    "db:reset-force:prod": "npm run reset-force:prod --workspace=@crit-fumble/core",
    "discord:register": "node scripts/register-discord-commands.js",
    "discord:register:force": "DISCORD_AUTO_REGISTER_COMMANDS=true node scripts/register-discord-commands.js",
    "discord:clear": "curl -X POST http://localhost:3000/api/discord/bot -H 'Content-Type: application/json' -d '{\"action\": \"clear_commands\"}'",
    "postinstall": ""
  },
  "dependencies": {
    "@crit-fumble/core": "file:./packages/core",
    "discord-interactions": "^4.3.0",
    "glob": "^11.0.3",
    "next": "^14.2.14",
    "node-loader": "^2.1.0",
    "react": "^18",
    "react-dom": "^18",
    "zlib-sync": "^0.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
